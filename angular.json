{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-bootstrap4-validate": {
      "projectType": "library",
      "root": "projects/angular-bootstrap4-validate",
      "sourceRoot": "projects/angular-bootstrap4-validate/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/angular-bootstrap4-validate/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/angular-bootstrap4-validate/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/angular-bootstrap4-validate/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/angular-bootstrap4-validate/**/*.ts",
              "projects/angular-bootstrap4-validate/**/*.html"
            ]
          }
        }
      }
    },
    "angular-bootstrap4-validate-demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/angular-bootstrap4-validate-demo",
      "sourceRoot": "projects/angular-bootstrap4-validate-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./extra-webpack.config.js"
            },
            "outputPath": "demo",
            "index": "projects/angular-bootstrap4-validate-demo/src/index.html",
            "main": "projects/angular-bootstrap4-validate-demo/src/main.ts",
            "polyfills": "projects/angular-bootstrap4-validate-demo/src/polyfills.ts",
            "tsConfig": "projects/angular-bootstrap4-validate-demo/tsconfig.app.json",
            "assets": [
              "projects/angular-bootstrap4-validate-demo/src/favicon.ico",
              "projects/angular-bootstrap4-validate-demo/src/assets"
            ],
            "styles": [
              "projects/angular-bootstrap4-validate-demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/angular-bootstrap4-validate-demo/src/environments/environment.ts",
                  "with": "projects/angular-bootstrap4-validate-demo/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angular-bootstrap4-validate-demo:build:production"
            },
            "development": {
              "browserTarget": "angular-bootstrap4-validate-demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-bootstrap4-validate-demo:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/angular-bootstrap4-validate-demo/**/*.ts",
              "projects/angular-bootstrap4-validate-demo/**/*.html"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "angular-bootstrap4-validate"
}
